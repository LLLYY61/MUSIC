image{
   width:100%; 
   height:100%;  
}
.header{
  border-bottom: 2rpx solid #d9d9d9;
  border-radius: 0rpx 0rpx 10rpx 10rpx;
  padding-bottom: 20rpx; 
  margin: 0 10rpx;
}
.head{
  border-left: 10rpx solid green;
  margin-left: 20rpx;
  margin-top: 30rpx;
  padding-left: 20rpx;
}
.list{
  margin-top: 10rpx;
  display:flex; 
  justify-content: space-around;
  flex-wrap: wrap;
}
.picture{ 
  padding-top: 20rpx;
}
.img{
  border-radius: 16rpx;
  width: 100%;  
  height: 200rpx;
}
.name{
  text-align: center;
  display: block; 
  font-size: 24rpx;
  width: 210rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}